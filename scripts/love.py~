#!/usr/bin/env python
from lib.bvehicle import BVehicle
import rospy

#Init
try:
    #Init ros node
    rospy.init_node('love')
    #Create the braitenberg vehicle
    bot = BVehicle('love')  
    #Start fear behaviour
    while bot.Running():
        bot.Run()
    rospy.spin()
except Exception as e:
    print(e)

